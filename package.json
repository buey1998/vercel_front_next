{
  "name": "frontend-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 4004",
    "dev:prod": "env-cmd -f .env.production next dev -p 4004",
    "build": "next build",
    "build:css": "yarn build:sass && npx tailwindcss -i ./src/styles/globals.css -o ./src/styles/output.css && yarn format:prettier-css",
    "build:tailwind": "tailwindcss -i ./src/styles/globals.css -o ./src/styles/output.css --watch",
    "build:sass-watch": "sass --no-source-map ./src/styles/scss:./src/styles/css --watch",
    "build:sass": "sass --no-source-map ./src/styles/scss:./src/styles/css",
    "start": "next start -p 40004",
    "uat": "next start -p 4008",
    "start:dev": "next start -p 4004",
    "build:dev": "env-cmd -f ./.env.development yarn build",
    "build:uat": "env-cmd -f ./.env.uat yarn build",
    "lint": "next lint --dir src",
    "format:eslint": "eslint --fix --ignore-path .eslintignore \"./**/*.{js,jsx,ts,tsx}\" && yarn format:prettier-tsx",
    "format:prettier": "prettier --write --ignore-path .prettierignore \"./**/*.{js,jsx,ts,tsx,json,css,scss, README.md}\"",
    "format:prettier-css": "prettier --write --ignore-path .prettierignore \"./**/*.{css,scss}\"",
    "format:prettier-tsx": "prettier --write --ignore-path .prettierignore \"./**/*.{js,jsx,ts,tsx}\"",
    "check:types": "tsc --pretty --noEmit",
    "check:prettier": "prettier --check .",
    "check:eslint": "eslint . --ext ts --ext tsx --ext js",
    "prepare": "husky install",
    "test": "jest --watch",
    "precommit": "git-format-staged --formatter \"prettier --stdin\" \"./**/*.{js,jsx,ts,tsx,json,css,scss}\"",
    "check-types": "tsc --pretty --noEmit",
    "check-lint": "eslint . --ext ts --ext tsx --ext js --max-warnings=0"
  },
